# ART
dalvik.vm.dex2oat-swap=false

# Display
# ro.hwui.texture_cache_size=24.0f
# ro.hwui.text_large_cache_width=2048
# ro.hwui.text_large_cache_height=512
ro.opengles.version=196609
ro.camera.rec.pixel_format=0x10F
ro.ycbcr.pixel_format=0x10F

#
# from device/xiaomi/latte/latte/system.prop
#
# Hwui Properties
ro.hwui.texture_cache_size=72
ro.hwui.layer_cache_size=48
ro.hwui.r_buffer_cache_size=34
ro.hwui.path_cache_size=40
ro.hwui.gradient_cache_size=1
ro.hwui.drop_shadow_cache_size=6
ro.hwui.texture_cache_flushrate=0.4
ro.hwui.text_small_cache_width=1024
ro.hwui.text_small_cache_height=1024
ro.hwui.text_large_cache_width=2048
ro.hwui.text_large_cache_height=2048

# DRM
drm.service.enabled=true

# Display
ro.sf.lcd_density=320

# Scheduler
sys.io.scheduler=bfq

#BT
ro.btconfig.if=uart
ro.btconfig.dev=/dev/ttyHSU0
ro.btconfig.vendor=broadcom
ro.btconfig.chip=BCM4356

#Wi-Fi
ro.wlan.mimo=1

#From MIUI getprop
persist.sys.job_delay=true
persist.sys.mcd_config_file=/system/etc/mcd_default.conf
persist.sys.perf.debug=true
